<%= form_for @employee do |f| %>
  <%= f.label :first_name %>
  <%= f.text_field :first_name %>
  <%= f.label "Last Name:" %>
  <%= f.text_field :last_name %>
  <%= f.label "Alias:" %>
  <%= f.text_field :alias %>
  <%= f.label "Title:" %>
  <%= f.text_field :title %>
  <%= f.label "Office" %>
  <%= f.text_field :office %>
  <%= f.collection_select('dog_was',@dogs,'id','name') %>
  <%= f.submit %>
<% end %>
