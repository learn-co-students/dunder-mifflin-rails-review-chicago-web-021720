<h1>Edit Dog</h1>

<%= form_for @dog do |f| %>
    <%= f.label :name %>
    <%= f.text_field :name %>
    <%= f.label :breed %>
    <%= f.text_field :breed %>
    <%= f.label :age %>
    <%= f.text_field :age %>
    <%= f.submit %>
<% end %>

<%= link_to "Delete", dog_path(@dog), method: "delete" %>
<%= link_to "Back", dogs_path %>